First Bad Version

class Solution {
public:
    int firstBadVersion(int n) {
        long long int start=0;
        long long int end=n;
        long long int minbad;
        while(end>=start)
        {
            long long int mid=(start+end)/2;
            if(isBadVersion(mid)==1)
            {
                minbad=mid;
                end=mid-1;

            }
            else{
                start=mid+1;
            }
        }
        return minbad;
    }
};
